#include<stdio.h>
int main()
{
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int m_ab = (a > b ? a : b);

    int m_abc = (m_ab > c ? m_ab : c);
    
    int max1 = (m_abc > d ? m_abc : d);

    int n_ab = (a < b ? a : b);
    int n_abc = (n_ab < c ? n_ab : c);
    
    int min1 = (n_abc < d ? n_abc : d);

    int x0 = (a == max1 ? min1 : a);
    int x1 = (b == max1 ? min1 : b);
    int x2 = (c == max1 ? min1 : c);
    int x3 = (d == max1 ? min1 : d);

    int m_x0x1 = (x0 > x1 ? x0 : x1);
    int m_x01x2 = (m_x0x1 > x2 ? m_x0x1 : x2);
    int second = (m_x01x2 > x3 ? m_x01x2 : x3);

    printf("%d\n", second);

	return 0;
}
