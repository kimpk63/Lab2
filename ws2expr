#include<stdio.h>
#include<math.h>
int main()
{
	int a=1,b=1,c=5,d=1,k;
	float ans1,ans2,ans4,ans5,ans3;
	ans1=3*((float)a/5)+(1/(float)b);
	printf("%f\n",ans1);
	
	ans2=(3*(float)a+5*(float)b)/(2+(float)c);
	printf("%f\n",ans2);

	ans3=(2.0/7)*(pow(4,(3+c))-(5*d));
	printf("%f\n",ans3);
	
	ans4=sqrt((2+8*b)/a);
	printf("%f\n",ans4);
	
	ans5=cbrt(pow(b,2)-4*d);
	printf("%f\n",ans5);

	return 0;
}
